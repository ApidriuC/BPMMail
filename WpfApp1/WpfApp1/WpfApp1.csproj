<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="dotnetbrowser.license" />
    <None Remove="favicon-0.png" />
    <None Remove="img\cerrar.png" />
    <None Remove="img\exit.png" />
    <None Remove="img\minimize.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="dotnetbrowser.license" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.ServiceModel.Duplex" Version="4.9.0" />
    <PackageReference Include="System.ServiceModel.Federation" Version="4.9.0" />
    <PackageReference Include="System.ServiceModel.Http" Version="4.9.0" />
    <PackageReference Include="System.ServiceModel.NetTcp" Version="4.9.0" />
    <PackageReference Include="System.ServiceModel.Security" Version="4.9.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="favicon-0.png" />
    <Resource Include="img\exit.png" />
    <Resource Include="img\minimize.png" />
  </ItemGroup>

</Project>
